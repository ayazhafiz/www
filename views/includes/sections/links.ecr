<% LINKS.each do |link| %>
<p>
  <span class="mr"><%= HTTP::Emoji.render link[:emoji], raw?: apple? %></span>
  <quote>
    <a href="<%= link[:link] %>"><%= link[:title] %></a>
  </quote>
  <% if link === LINKS.last? %>
  <br />
  <small>If you wanna edit or view the "secret" APIs of this website, check out
  <a href="https://github.com/ayazhafiz/crystal-ah">the source</a>.</small>
  <% end %>
</p>
<% end %>
