{
  "name": "cr-ayazhafiz.com",
  "version": "0.0.1",
  "description": "personal site",
  "main": "index.js",
  "scripts": {
    "precompile:css": "node-sass src/css/index.scss -o public/css/ --output-style compressed",
    "compile:css": "postcss public/css/*.css --use autoprefixer -d public/css/ --no-map",
    "build": "npm-run-all compile:css",
    "deploy": "./app --port $PORT",
    "deploy:dev": "./sentry",
    "start": "npm-run-all build deploy",
    "start:dev": "npm-run-all build deploy:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayazhafiz/crystal-ah.git"
  },
  "author": "Hafiz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayazhafiz/crystal-ah/issues"
  },
  "homepage": "https://github.com/ayazhafiz/crystal-ah#readme",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.0.0"
  },
  "engines": {
    "node": "7.9.0",
    "npm": "4.2.0"
  }
}