{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "ES2016", "ES2017", "DOM"],
    "noImplicitAny": false,
    "sourceMap": false,
    "alwaysStrict": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}
